{
  "$schema": "http://json.schemastore.org/template",
  "author": "FatTiger",
  "classifications": [ "Web", "Extension", "Three-Tier Architecture" ],
  "identity": "TTA.Extension.Template",
  "name": "Clean Three-Tier Architecture Extension",
  "shortName": "tta-extension",
  "sourceName": "Extension.Template",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            "**/[Bb]in/**",
            "**/[Oo]bj/**",
            ".template.config/**/*",
            "**/*.filelist",
            "**/*.user",
            "**/*.lock.json",
            "**/.git/**",
            "**/LICENSE",
            "**/*.md",
            "**/.vs/**",
            "**/.gitignore"
          ]
        }
      ]
    }
  ]
}